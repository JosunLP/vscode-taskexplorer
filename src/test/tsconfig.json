{
    "extends": "../../tsconfig.json",
    "compilerOptions": {
		"isolatedModules": false,
        "outDir": "../../dist/test",
		"baseUrl": ".",
		"rootDir": ".",
		"sourceMap": false,
		"inlineSourceMap": true,
		"tsBuildInfoFile": "../../node_modules/.cache/tsconfig.test.tsbuildinfo",
        "paths": {
			// "interface/*": [ "../../types" ],
			":env/*": [ "../../src/lib/env/web/*", "../../src/webview/common/env/web/*" ],
			":types": [ "../../types" ]
		} // ,
		// "typeRoots": [ "../../types/dist" ]
    },
	"include": [
		"./**/*"
	],
    "exclude": [
        "**/.coverage",
        "**/.nyc_output",
        "**/.vscode",
        "**/.vscode-test",
        "**/dist",
        "**/doc",
        "**/node_modules",
        "**/res",
        "**/script",
        "**/src/webview/app/**",
        "**/src/lib/env/web/**",
        "**/src/webview/common/env/web/**",
        "**/test-fixture"
    ]
}