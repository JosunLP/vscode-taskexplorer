{
    "name": "vscode-taskexplorer",
    "displayName": "Task Explorer",
    "detailedDisplayName": "Task Explorer VSCode Extension",
    "publicInfoProject": true,
    "builds": {
        "dev": [
            { "build": "extension" },
            { "build": "webview" }
        ],
        "testprod": [
            { "build": "extension" },
            { "build": "webview", "environment": "prod "}
        ],
        "prod": [
            { "build": "extension" },
            { "build": "webview" }
        ],
        "test": [
            { "build": "extension" },
            { "build": "webview", "environment": "dev "}
        ]
    },
    "colors": {
        "default": "grey",
        "buildBracket": "blue",
        "buildText": "white",
        "valueStar": "cyan",
        "valueStarText": "white",
        "tagBracket": "blue",
        "tagText": "white",
        "uploadSymbol": "yellow"
    },
    "log": {
        "level": 3,
        "pad": {
            "basePad": 0,
            "envTag": 23,
            "value": 30,
            "uploadFileName": 50
        }
    },
    "paths": {
        "build": ".",
        "cache": "node_modules/.cache/wpbuild",
        "dist": "dist",
        "distTests": "dist/test",
        "temp": null,
        "files": {
            "hashStore": "",
            "sourceMapWasm": ""
        }
    },
    "exports": {
        "cache": true,
        "context": true,
        "devtool": true,
        "entry": true,
        "experiments": true,
        "externals": true,
        "ignorewarnings": true,
        "index": true,
        "minification": false,
        "mode": true,
        "name": true,
        "optimization": true,
        "output": true,
        "plugins": true,
        "resolve": true,
        "rules": true,
        "stats": true,
        "target": true,
        "watch": true
    },
    "plugins": {
        "analyze": true,
        "banner": true,
        "build": true,
        "clean": true,
        "compilae": true,
        "copy": true,
        "environment": true,
        "hash": true,
        "html": true,
        "ignore": false,
        "index": true,
        "instrument": true,
        "licensefiles": true,
        "loghooks": true,
        "optimization": true,
        "progress": false,
        "runtimevars": true,
        "scm": true,
        "sourcemaps": true,
        "tscheck": true,
        "upload": true,
        "vendormod": true
    },
    "vscode": {
        "webview": {
            "baseDir": "src/webview/app",
            "apps": {
                "home": "./home/home.ts",
                "license": "./license/license.ts",
                "monitor": "./monitor/monitor.tsx",
                "parsingReport": "./parsingReport/parsingReport.ts",
                "releaseNotes": "./releaseNotes/releaseNotes.ts",
                "taskCount": "./taskCount/taskCount.ts",
                "taskDetails": "./taskDetails/taskDetails.ts",
                "taskUsage": "./taskUsage/taskUsage.ts",
                "welcome": "./welcome/welcome.ts"
            }
        }
    }
}